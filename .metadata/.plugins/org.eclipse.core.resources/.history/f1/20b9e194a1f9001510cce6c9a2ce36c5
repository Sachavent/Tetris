package Launcher;

import InterfaceGraphique.Fenetre;

public class InitialisationJoueur {

	public static void initialisationJoueur (int numerojoueur) {

		switch (numerojoueur) {
		case 0:
			return Piece1;
		case 1:
			return Piece2;
		case 2:
			return Piece3;
		default: return null;
		}
	}
	
	
	
	
}
