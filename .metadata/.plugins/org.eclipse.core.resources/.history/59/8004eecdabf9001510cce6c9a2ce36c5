package GestionFichier;

import java.net.URL;

public class LoadRessource {
	  static LoadRessource lr= new LoadRessource();
    public static String loadRessource()
    {	
    	System.out.println("chemin : " +lr.getClass().getResource("src/Scores.txt"));
        return lr.class.getResource("/resources/textfile.txt").getPath();;
    }
}
